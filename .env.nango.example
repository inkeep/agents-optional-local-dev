# Nango Configuration for Docker Compose
# Copy this file to .env and update the values below

# REQUIRED: BASE64-256BIT-KEY encryption key (generate with: openssl rand -base64 32)
NANGO_ENCRYPTION_KEY=REPLACE_WITH_BASE64_256BIT_ENCRYPTION_KEY

# Dashboard Authentication (leave empty for no auth in development)
NANGO_DASHBOARD_USERNAME=
NANGO_DASHBOARD_PASSWORD=

# Database Configuration (defaults work for local development)
NANGO_DB_USER=nango
NANGO_DB_PASSWORD=nango
NANGO_DB_NAME=nango

# Optional: Logging and Features (defaults shown below)
# NANGO_LOG_LEVEL=info
# NANGO_FLAG_SERVE_CONNECT_UI=true
# NANGO_FLAG_AUTH_ENABLED=false # This will disable regular login and signup